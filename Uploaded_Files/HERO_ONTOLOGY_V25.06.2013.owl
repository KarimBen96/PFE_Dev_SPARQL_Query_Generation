    <owl:Class rdf:about="http://www.UniversityReferenceOntology.org/HERO#Course">
        <rdfs:subClassOf rdf:resource="http://www.UniversityReferenceOntology.org/HERO#Deliverable"/>
    </owl:Class>
    


    <!-- http://www.UniversityReferenceOntology.org/HERO#Dean -->

    <owl:Class rdf:about="http://www.UniversityReferenceOntology.org/HERO#Dean">
        <rdfs:subClassOf rdf:resource="http://www.UniversityReferenceOntology.org/HERO#TechnicalStaff"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.UniversityReferenceOntology.org/HERO#HasDegree"/>
                <owl:hasValue rdf:resource="http://www.UniversityReferenceOntology.org/HERO#doctorate"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:isDefinedBy xml:lang="en">At universities with separate schools or colleges, these units usually are headed by a dean who reports to the chief academic officer and oversees the work of the departments that make up the school or college. Deans, especially those of large colleges or those who bring in considerable funding from external sources, may enjoy substantial autonomy.</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Criteria to appoint a dean: variable such as :has PHD degree, has prior positions in faculty staff, average age (60 years old)…</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.UniversityReferenceOntology.org/HERO#Degree -->

    <owl:Class rdf:about="http://www.UniversityReferenceOntology.org/HERO#Degree">
        <rdfs:subClassOf rdf:resource="http://www.UniversityReferenceOntology.org/HERO#Deliverable"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.UniversityReferenceOntology.org/HERO#ObtainedBy"/>
