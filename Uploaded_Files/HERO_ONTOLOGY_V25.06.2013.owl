    <owl:DatatypeProperty rdf:about="http://www.UniversityReferenceOntology.org/HERO#UniversityResearchRating">
        <rdfs:domain rdf:resource="http://www.UniversityReferenceOntology.org/HERO#HigherEducationOrganization"/>
